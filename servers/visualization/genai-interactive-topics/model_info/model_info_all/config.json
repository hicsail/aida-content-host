{
    "topic_model": {
        "nr_topics": "auto",
        "language": "english",
        "top_n_words": 10,
        "min_topic_size": 6,
        "verbose": true,
        "calculate_probabilities": true
    },
    "umap": {
        "n_neighbors": 15,
        "n_components": 5,
        "min_dist": 0.0,
        "metric": "cosine",
        "random_state": 42
    },
    "hdbscan": {
        "min_cluster_size": 15,
        "min_samples": 5,
        "metric": "euclidean",
        "cluster_selection_method": "eom"
    },
    "vectorizer": {
        "analyzer": "word",
        "stop_words": "english"
    },
    "plot": {
        "intertopic_height": 700,
        "intertopic_width": 700,
        "term_bar_height": 400
    }
}